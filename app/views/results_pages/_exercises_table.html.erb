
	<h3><%= e.name %></h3>
	
	<% if e.comment %>
		<%= e.comment %>
	<% end %>
	
	<% if e.strength && e.strength != 0  %>
			<span data-tooltip class="has-tip" title="Personal Record, 1-rep maximum (Only calculated for 10 reps or less due to accuracy)">
				<div style="color:red;">
				PR: <%= number_with_precision(e.strength, precision:1) %>
				</div>
			</span>
	<% end %>
	<br>
	<% e.segments.each do |s| %>
	<ul>
		<li>
			<%= render 'segments_table', s: s %>
		</li>
	</ul>
	<% end %>
